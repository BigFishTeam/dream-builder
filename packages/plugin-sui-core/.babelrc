{
  "presets": ["react-app"],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "import",
      { "libraryName": "antd", "libraryDirectory": "lib", "style": true }
    ],
    [
      "babel-plugin-sui-import",
      {
        "libraryName": "sui-components",
        "libraryDirectory": "components"
      }
    ]
  ]
}
