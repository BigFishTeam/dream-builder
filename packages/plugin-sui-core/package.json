    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --config .prettierrc --write",
      "eslint src/core --fix",
      "git add"
    ],
    "*.{md,css,less,html}": [
      "prettier --config .prettierrc --write",
      "git add"
    ],
    "yarn.lock": [
      "git rm --cached"
    ],
    "package-lock.json": [
      "git rm --cached"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
